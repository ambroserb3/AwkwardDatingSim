	<title>Awkward Dating Sim</title>
    
	<!-- Fonts, Fontawesome Bootstrap, Core CSS, -->
	<!-- <link href="../../stylesheets/fonts.css" rel="stylesheet" type="text/css" /> -->
	<!-- <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous"> -->
	<link href="../../stylesheets/bootstrap.min.css" rel="stylesheet" />
	<link href="../../stylesheets/rooms.css" rel="stylesheet" type="text/css" />
	
    <script src="/socket.io/socket.io.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="scripts/lobby.js"></script>

</head>
<body>
    <div class="bg">
        <div class="container">
            <div class="row">
                <div id="roomslist" class="col-xs-2">
                    <b>ROOMS TO JOIN</b>
                    <div id="rooms"></div>
                </div>
                <div id="roomcontainer" class="col-xs-7">
                    <div id="room creation">
                    <input id="roomname" style="width:200px;" placeholder="Room name" class="form-control"/>
                        <input type="button" id="roombutton" value="Create Room" class="btn" />
                        </div>
                </div>
                <div id="chatcontainer" class="col-xs-3">
                    Chat! (Only works if you join a room)
                    <div id="conversation">
                        <input id="data" class="form-control" />
                        <input type="button" id="datasend" value="send" class="btn btn-outline-secondary"/>
                    </div>
                </div>
            </div>
            <div id="room panel" class="roomstart">
                Players in room <b id="myroom">Lobby</b>:
                <p id="p1" class="player"></p>
                <!-- <p id="p2" class="player">player 2</p> -->
                <div id="startdatecontainer">
                    <input type="button" id="begindate" value="Start Date" class="btn"/>
                </div>
            </div>
        </div>
    </div>
</body>